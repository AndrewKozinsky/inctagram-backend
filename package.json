{
	"name": "inctagram-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build:main": "nest build",
		"start:main": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/apps/main/main",
		"test": "NODE_ENV=TEST jest --config apps/main/test/integration/jest.json --runInBand",
		"test:e2e": "NODE_ENV=TEST jest --config apps/main/test/e2e/jest-e2e.json --runInBand",
		"test:watch": "NODE_ENV=TEST jest --watch --runInBand",
		"test:cov": "NODE_ENV=TEST jest --coverage",
		"migrate:dev": "prisma migrate dev",
		"migrate:generate-types": "prisma generate"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.0.0",
		"@nestjs/cqrs": "^10.2.7",
		"@nestjs/platform-express": "^10.0.0",
		"@prisma/client": "5.18.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cookie-parser": "^1.4.6",
		"prisma": "^5.18.0",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.5.0",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/apps/",
			"<rootDir>/libs/"
		],
		"moduleNameMapper": {
			"^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1"
		}
	}
}
