{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "paths": {
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/server-helper": [
        "libs/server-helper/src"
      ],
      "@app/server-helper/*": [
        "libs/server-helper/src/*"
      ],
      "@app/email-adapter": [
        "libs/email-adapter/src"
      ],
      "@app/email-adapter/*": [
        "libs/email-adapter/src/*"
      ],
      "@app/hash-adapter": [
        "libs/hash-adapter/src"
      ],
      "@app/hash-adapter/*": [
        "libs/hash-adapter/src/*"
      ],
      "@app/browser-service": [
        "libs/browser-service/src"
      ],
      "@app/browser-service/*": [
        "libs/browser-service/src/*"
      ],
      "@app/jwt-adapter": [
        "libs/jwt-adapter/src"
      ],
      "@app/jwt-adapter/*": [
        "libs/jwt-adapter/src/*"
      ],
      "@app/re-captcha-adapter": [
        "libs/re-captcha-adapter/src"
      ],
      "@app/re-captcha-adapter/*": [
        "libs/re-captcha-adapter/src/*"
      ]
    },
  }
}
