{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "server-helper": {
      "type": "library",
      "root": "libs/server-helper",
      "entryFile": "index",
      "sourceRoot": "libs/server-helper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/server-helper/tsconfig.lib.json"
      }
    },
    "email-adapter": {
      "type": "library",
      "root": "libs/email-adapter",
      "entryFile": "index",
      "sourceRoot": "libs/email-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/email-adapter/tsconfig.lib.json"
      }
    },
    "hash-adapter": {
      "type": "library",
      "root": "libs/hash-adapter",
      "entryFile": "index",
      "sourceRoot": "libs/hash-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/hash-adapter/tsconfig.lib.json"
      }
    },
    "browser-service": {
      "type": "library",
      "root": "libs/browser-service",
      "entryFile": "index",
      "sourceRoot": "libs/browser-service/src",
      "compilerOptions": {
        "tsConfigPath": "libs/browser-service/tsconfig.lib.json"
      }
    },
    "jwt-adapter": {
      "type": "library",
      "root": "libs/jwt-adapter",
      "entryFile": "index",
      "sourceRoot": "libs/jwt-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt-adapter/tsconfig.lib.json"
      }
    },
    "re-captcha-adapter": {
      "type": "library",
      "root": "libs/re-captcha-adapter",
      "entryFile": "index",
      "sourceRoot": "libs/re-captcha-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/re-captcha-adapter/tsconfig.lib.json"
      }
    }
  }
}
